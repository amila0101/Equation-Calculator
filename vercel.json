{
  "version": 2,
  "installCommand": "cd frontend && npm ci && pip install -r requirements.txt",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/build",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index" }
  ],
  "functions": {
    "api/index.py": {
      "runtime": "python3.9"
    }
  }
}
